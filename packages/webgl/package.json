{
  "name": "@stemngine/webgl",
  "version": "1.0.0",
  "description": "WebGL software simulation",
  "main": "dist/webgl.umd.js",
  "module": "dist/webgl.esm.js",
  "types": "dist/types/webgl.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/webgl.d.ts",
      "import": "./dist/webgl.esm.js",
      "require": "./dist/webgl.umd.js"
    }
  },
  "scripts": {
    "build:dev": "rollup -c rollup.dev.config.js",
    "build:dev:watch": "rollup -c -w rollup.dev.config.js",
    "build:prod": "rollup -c rollup.prod.config.js",
    "types:dev": "tsc -p tsconfig.json --watch",
    "types:prod": "tsc -p tsconfig.json",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.17.1"
}
